# Recipe (MakeRecipe) for QEMU by Lucas Correia Villa Real <lucasvr@gobolinux.org>, on Sun Oct 17 23:20:23 BRST 2004
compile_version=1.8.2
url="http://wiki.qemu.org/download/qemu-2.7.0.tar.bz2"
file_size=26867760
file_md5=08d4d06d1cb598efecd796137f4844ab
recipe_type=configure

environment=(
    CFLAGS=
    CXXFLAGS=
)

using_spice() {
   configure_options=( "${configure_options[@]}" "--enable-spice" )
}

configure_options=(
    --enable-system
    --enable-linux-user
    --enable-kvm
)
